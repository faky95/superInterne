<div class="modal-header">
	<button type="button" class="close" data-dismiss="modal">
		<span class="icon12 minia-icon-close"></span>
	</button>
	<h4 align="left">
		<span class="icon16 icomoon-icon-earth"></span>
       	<span>Orientation vers une action générique</span>
	</h4>
</div>
<form class="form-horizontal" action="{{ path('orienter_action', {'data': data }) }}" method="POST" novalidate="validate" role="form">
	<div class="panel-body">
		{{ form_row(form.actionGenerique,  {'attr': {'class': 'form-control'}}) }}
		{{ form_widget(form._token) }}
		<!-- End .form-group  -->
	</div>
	<div class="form-actions">
		<div class="col-lg-offset-2 col-lg-9">
			<a data-target="#myModal" class="btn btn-success submitLink" call="redirectModalToUrl">Confirmer</a>
			<a class="btn btn-primary" data-dismiss="modal">Fermer</a>
		</div>
	</div>
</form>
