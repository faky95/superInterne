              <div class="form-group">
                  <label class="col-lg-2 control-label" for="checkboxes">Numérateur</label>
                       <div class="col-lg-10">
                       {% set i=1 %}
			             {% for reference in references %}
			               <label class="checkbox-inline">
			             	 <input type="checkbox" id="numerateur" value="{{ i }}" >
			                 {{ reference.symbole }}
			                 {% set i=i+1%}
			               </label>
			             {% endfor %}
			             </div>
             </div>
